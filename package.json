{
  "name": "react-webpack-babel",
  "version": "0.0.4",
  "description": "React Webpack Babel Starter Kit",
  "main": "''",
  "scripts": {
    "build": "webpack --config webpack.dev.config.js --progress --profile --colors ",
    "build:test": "webpack --config webpack.test.config.js --progress --profile --colors ",
    "build:preprod": "webpack --config webpack.pre-prod.config.js --progress --profile --colors ",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors ",
    "start": "webpack-dev-server --progress --profile --colors",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": " webpack-dashboard -- webpack-dev-server",
    "test": "SET NODE_ENV=test && jest",
    "ssr": "SET NODE_ENV=production && node ./server.js",
    "copy": "node ./copyWebconfig.js",
    "e2e-chrome": "nightwatch --config nightwatch.conf.BASIC.js",
    "e2e-edge": "nightwatch --config nightwatch.conf.EDGE.js",
    "e2e-firefox": "nightwatch --config nightwatch.conf.GECKO.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alicoding/react-webpack-babel"
  },
  "author": "Ali Al Dallal",
  "license": "MIT",
  "homepage": "https://github.com/alicoding/react-webpack-babel#readme",
  "dependencies": {
    "express": "^4.16.2",
    "file-system": "^2.2.2",
    "fs-extra": "^4.0.2",
    "node-sass": "^4.3.0",
    "nuka-carousel": "^3.0.0",
    "prop-types": "^15.6.0",
    "react": "15.6.1",
    "react-burger-menu": "^2.1.11",
    "react-dom": "^15.4.2",
    "react-dropdown": "^1.3.2",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-runtime": "^6.22.0",
    "chromedriver": "^2.33.2",
    "copy-webpack-plugin": "^4.2.1",
    "css-loader": "0.26.1",
    "dotenv": "^4.0.0",
    "edgedriver": "^4.15063.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.3.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "fetch-mock": "^5.13.1",
    "file-loader": "^0.10.0",
    "geckodriver": "^1.10.0",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^20.0.4",
    "loglevel": "^1.4.1",
    "nightwatch": "^0.9.19",
    "postcss-loader": "^1.2.2",
    "prop-types": "^15.6.0",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "selenium-download": "^2.0.10",
    "selenium-server": "^3.8.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "^3.3.0",
    "webpack-cleanup-plugin": "^0.4.2",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.1.1",
    "xmlhttprequest": "^1.8.0"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js",
      "**/*.jsx",
      "!**/Pdp/index.jsx",
      "!**/Plp/index.jsx",
      "!postcss.config.js",
      "!server.js",
      "!webpack.config.js",
      "!webpack.loaders.js",
      "!webpack.production.config.js",
      "!**/prettify.js",
      "!**/sorter.js",
      "!**/3764d345cb049707d5cf.js",
      "!src/index.jsx",
      "!public"
    ],
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff2?|mp[34]|webm|wav|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|s[ac]ss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  }
}
